{
  "name": "devsu-website-2021",
  "version": "1.0.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build && next export",
    "start": "next start",
    "format": "prettier --config .prettierrc \"./**/*.{js,css}\" --write",
    "lint": "eslint \"./\"",
    "lint:fix": "npm run lint --fix",
    "validate": "npm run format && npm run lint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,css}": [
      "npm run format",
      "npm run lint:fix"
    ]
  },
  "dependencies": {
    "autoprefixer": "^10.0.4",
    "next": "^10.0.3",
    "next-routes": "^1.4.2",
    "postcss": "^8.1.14",
    "prismic-dom": "^2.1.0",
    "prismic-javascript": "^2.7.1",
    "prismic-reactjs": "^1.3.3",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "tailwindcss": "^2.0.1"
  },
  "devDependencies": {
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.2.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.2",
    "prettier": "2.2.1"
  },
  "engines": {
    "node": "12.14.x"
  }
}
